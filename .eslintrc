{
  "settings": {
    "import/resolver": {
      "alias": [
        ["@components", "./src/components"],
        ["@styles", "./src/styles"],
        ["~types", "./types"]
      ]
    }
  },

  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },

  "env": {
    "es6": true
  }
}
